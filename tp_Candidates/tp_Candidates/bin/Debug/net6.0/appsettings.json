{
  // подключение базы данных
  "ConnectionStrings": {
    // используем LocalDB (легковесная версия SQL Server Express)
    // БД называется applicationdb
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=applicationdb;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}